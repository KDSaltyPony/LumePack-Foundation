{
    "name": "lume-pack/foundation",
    "description": "Some fundamental lumen classes for a cool REST API",
    "authors": [
        {
            "name": "KDSaltyPony",
            "email": "kallofdragon@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^10",
        "laravel/sanctum": "^3",
        "mongodb/laravel-mongodb": "^4"
    },
    "autoload": {
        "psr-4": {
            "LumePack\\Foundation\\": "src",
            "LumePack\\Foundation\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LumePack\\Foundation\\Providers\\LaravelServiceProvider",
                "LumePack\\Foundation\\Providers\\EventServiceProvider"
            ]
        }
    }
}
