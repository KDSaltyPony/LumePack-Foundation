{
    "name": "lume-pack/foundation",
    "description": "Some fundamental lumen classes for a cool REST API",
    "authors": [
        {
            "name": "KDSaltyPony",
            "email": "kallofdragon@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "php": ">=7.2.19",
        "laravel/lumen-framework": "^7.0.5|^8.0",
        "illuminate/database": "^7.30.4|^8.0",
        "illuminate/http": "^7.30.4|^8.0",
        "illuminate/pagination": "^7.30.4|^8.0",
        "illuminate/support": "^7.30.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "LumePack\\Foundation\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "LumePack\\Foundation\\Providers\\LumenServiceProvider"
            ]
        }
    }
}
